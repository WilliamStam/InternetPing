<html class="no-js"> <!--<![endif]-->
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>{{ page['meta']['title'] }}</title>
	<meta name="description" content="">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
	
	
	<link rel="stylesheet" href="/app/style.{{ _v }}.css">
	{% for row in page['css'] %}
		<link rel="stylesheet" href="{{ row }}">
	{% endfor %}
	{% if page['template_css'] %}
		<link rel="stylesheet" href="{{ page['template_css'] }}">
	{% endif %}
	
	
	
	
	<script src="/vendor/components/modernizr/modernizr.{{ _v }}.js"></script>
</head>
<body id="body" >


<section id="nav-area">
{% include "_header.twig" %}
</section>
<section id="page-area">
	{% include page['template'] %}
</section>
<section id="footer-area">
{% include "_footer.twig" %}
</section>
	
	
	
	
	
	
<script src="/app/javascript.{{ _v }}.js"></script>

{% for row in page['js'] %}
	<script src="{{ row }}"></script>
{% endfor %}
{% if page['template_js'] %}
	<script src="{{ page['template_js'] }}"></script>
{% endif %}




</body>
</html>